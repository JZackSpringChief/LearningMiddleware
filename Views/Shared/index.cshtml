@using refreashOnCsharp.Models

<!DOCTYPE html>
<html>
    <head>
        <title>
            @ViewData["appTitle"]
        </title>
        <meta carset="utf-8" />
    </head>
    <body>
        <h1>Welcome</h1>
        @{
            List<Person3>? people = (List<Person3>)ViewData["people"];
        }
        @foreach(Person3 person in people) 
        {
            <div>@person.Name, </div>
            <span> @person.PersonGender</span>
            @if (person.DateOfBirth != null)
            {
                <span>@person.DateOfBirth.Value.ToString("MM/dd/yyyy")</span>

            }
        }
       
    </body>
</html>